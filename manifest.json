{
	"applications": {
  	"gecko": {
    	"id": "torpedo-browser@tu-darmstadt.de"
  	}
	},
	"manifest_version":2,
	"name":"__MSG_extensionName__",
	"version":"3.0",
	"default_locale": "en",
	"description":"__MSG_extensionDescription__",
	"background":{
		"scripts":[
			"js/jquery-2.1.1.min.js",
			"js/preferences.js",
			"js/options.js",
			"background.js"
		]
	},
	"permissions":[
		"tabs",
		"<all_urls>",
		"webRequest",
		"webRequestBlocking",
		"storage"
	],
	"page_action": {
		"browser_style": true,
		"default_icon": {
	    "38": "img/icon38.png",
	    "64": "img/icon64.png"
	  },
	  "default_title": "__MSG_extensionName__",
		"default_popup": "icon.html"
  },
	"options_ui":{
		"page": "options.html",
		"browser_style": true
	},
	"icons":{
		"38":"img/icon38.png",
		"64":"img/icon64.png"
	},
	"web_accessible_resources": [
		"img/*.png"
	],
	"content_scripts":[
		{
		  "matches": [
		  	"https://mail.google.com/*",
				"https://mg.mail.yahoo.com/*",
				"https://email.t-online.de/*",
				"https://outlook.live.com/*",
				"https://*.web.de/*",
				"https://*.gmx.net/*"
		  ],
		  "css": [
		  	"css/jquery.qtip.min.css",
				"css/tooltip.css"
		  ],
		  "js": [
		  	"js/jquery-2.1.1.min.js",
		  	"js/jquery.qtip.min.js",
		  	"js/contentscript.js",
		  	"js/timer.js",
				"js/tooltip.js",
		  	"js/status.js",
		  	"js/redirect.js",
		  	"js/storage.js"
		  ],
		  "run_at": "document_idle",
			"all_frames": true
		}
	]
}
